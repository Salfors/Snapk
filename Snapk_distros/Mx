function Mx() {
	case $TODO in 
		"1")
			snap_note="\n${SM} Snap does not support your distro ${EM}\n"  ;;

		"2")
			flapak_note="\n${SM} your distribution might be out of date and need an upgrade ${EM}\n" ;;

		"3")
			snap_note="\n${SM} Snap does not support your distro ${EM}\n" 
			flapak_note="\n${SM} your distribution might be out of date and need an upgrade ${EM}\n" ;;
	esac
}
